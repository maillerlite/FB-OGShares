{
  "name": "fb-ogshares",
  "version": "1.0.0",
  "description": "Generator sharing Facebook App link",
  "main": "index.js",
  "engines": {
    "node": ">=8.11.3"
  },
  "scripts": {
    "nodemon": "node_modules/.bin/nodemon --inspect --exec 'node -r dotenv/config' server.js --config nodemon.json",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fahri5567/FB-OGShares.git"
  },
  "author": "Mas Fahri <fahri372a@gmail.com> (http://mas-fahri.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fahri5567/FB-OGShares/issues"
  },
  "homepage": "https://github.com/Fahri5567/FB-OGShares#readme",
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "core-fb-ogshares": "^1.0.0",
    "cross-env": "^5.2.0",
    "font-awesome": "^4.7.0",
    "laravel-mix": "^2.1.14",
    "nodemon": "^1.18.4",
    "normalize.css": "^8.0.0",
    "tippy.js": "^3.0.1"
  }
}
